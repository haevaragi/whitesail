					MiniSite.Cookie.set('QQ_IPAddress',IPData[2]+','+IPData[3]);
					MiniSite.Home._print(IPData[2]);
				}else{
					MiniSite.Home._print();
				}
			});
		}else{
			var ipAddress=MiniSite.Cookie.get("QQ_IPAddress");
			var ipAddressArr=ipAddress.split(",");
