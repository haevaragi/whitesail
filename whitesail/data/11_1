<script language=JavaScript> 
function showList(listId){
	for(var i=1;i<=10;i++){ 
		if(i==listId){
			document.getElementById("multilist_"+listId).style.display="block";
			document.getElementById("multilist_nav_"+listId).className="multilist_hover";
		}
		else{
			document.getElementById("multilist_"+i).style.display="none";
			document.getElementById("multilist_nav_"+i).className="multilist_nav";
		}
	}
} 
</script>
