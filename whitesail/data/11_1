function GetHostRequest()
{
    var info= document.form_wm.domain.value;

    if (!document.form_wm.domain.value)
        return false;

    if (g_server.length>0 && g_domain==document.form_wm.domain.value)
        return false;
    else
        g_domain = document.form_wm.domain.value;

    document.getElementById("trans").src = "http://wmdns.263.net/req.html#"+escape(info);
    try{
        setTimeout("ff_reload()",100);
    }
    catch (e)
    {
        //alert(e.name+",msg="+e.message);
    }
}
function ma_login_close()
{
    document.getElementById("notlog").style.display="none";
    document.getElementById("user").value = "";
    document.getElementById("pswd").value = "";
    document.body.style.overflow ="auto";

}
function ma_login()
{
	document.body.style.overflow ="hidden";
	document.getElementById("notlog").style.height = screen.height + "px";
	document.getElementById("notlog").style.width = screen.width + "px";
    document.getElementById("notlog").style.display="block";	
    document.getElementById("user").value = "";
    document.getElementById("pswd").value = "";
}
function GetMaHostRequest()
{
    var info= document.form_ma.user.value;
    var a = info.split("@");
    var b = unescape(a[ a.length-1]);
    
    document.getElementById("trans").src = "http://wmdns.263.net/req.html#"+escape(b);
    try{
        setTimeout("ff_reload()",100);
    }
    catch (e)
    {
        //alert(e.name+",msg="+e.message);
    }
}
function ff_reload()
{
    try{
    window.transname.location.reload();
    }
    catch (e)
    {
    }
}
