<script type="text/javascript">
function checkemail(form)
{
  if(!form.LoginName.value){
    form.LoginName.focus();
    return false;
  }
  if(!form.passwd.value){
    form.passwd.focus();
	return false;
  }
  var email = form.LoginName.value;
  var current = form.DomainName.value;
  if (email.lastIndexOf("@")==-1) {
	email = email + "@" + current;
  }
  form.UserName.value = email;
  /*frmmail.action="http://passport.21cn.com/maillogin.jsp";*/
  if(form.DomainName[1].selected){
    form.DomainName.value="21cn.net";
    //form.action="http://webmail.21cn.net/nature/gb/NULL/NULL/NULL/SignIn.gen";
	form.action="http://hermes.webmail.21cn.net/webmail/business_router.jsp";
  }else{
    //form.action="http://webmail.21cn.com/NULL/NULL/NULL/NULL/NULL/SignIn.gen";
	form.action="http://passport.21cn.com/maillogin.jsp";
  }
  setTimeout('clearpsw()',100);
}
function clearpsw(){
	document.frmmail.passwd.value="";

}
</script>
