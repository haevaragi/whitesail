<script>
	function upAndDown(){
		document.location.reload();
	}
function setCookie (name, value, expires) {
	document.cookie = name + "=" + escape(value) + 
	"; expires=" + expires.toGMTString() + "; path=/"; 
}
function deleteCookie(name) 
{ 
	var expdate = new Date(); 
	expdate.setTime(expdate.getTime() - (1*60000*30)); 
	setCookie(name, "", expdate); 
} 
function replyboxShow(backurl){
	//backurl;
	document.getElementById("replyboxurl").value = backurl;
	document.getElementById("replyComment").value = "";
	winBGShow();
	var replaybox;
	if (document.getElementById("replybox")) {
		replaybox = document.getElementById("replybox");
	} else {
		replaybox = document.createElement("div");
		document.body.appendChild(replaybox);
		replaybox.id = "replybox";
		replaybox.className = "replybox";
	}
	replaybox.innerHTML =  document.getElementById("replybox2").innerHTML;
		document.getElementById("replybox2").innerHTML = "";
	replaybox.style.display = "block";
	with(replaybox.style){
		/*
		zIndex = 102;
		position = "absolute";
		var scrollTop = document.body.scrollTop + document.documentElement.scrollTop;
		left = (document.documentElement.clientWidth - replaybox.offsetWidth) / 2 + 'px';
		top = (scrollTop + 150) + 'px';
		*/
		zIndex = 112;
		position = "absolute";
		var objDiv = document.getElementById(backurl); 
		var docScrollLeft = document.body.scrollLeft; 
		var docSrollTop = document.body.scrollTop; 
		var e = objDiv; 
		var t = e.offsetTop; 
		var l = e.offsetLeft; 
		while(e = e.offsetParent){ 
		t += e.offsetTop; 
		l += e.offsetLeft; 
		} 
		l=l+60;
		t=t+80;
		left = l+"px";
		top = t+"px";
	}
         document.getElementById("replyComment").focus();
}

//背景层
function winBGShow(){
	var bgId = "divWinBG";
	if (document.getElementById(bgId))
	{
		document.getElementById(bgId).style.display='block';
	} else {
		var divBG = document.createElement("div");
		divBG.id = bgId;
		divBG.className = "";
		document.body.appendChild(divBG);
		with(divBG.style){
			var bodySize = [];
			with(document.documentElement) {
				bodySize[0] = (scrollWidth>clientWidth) ? scrollWidth : clientWidth;
				bodySize[1] = (scrollHeight>clientHeight) ? scrollHeight : clientHeight;
			}
			width = bodySize[0] + 'px';
			height = bodySize[1] + 'px';
			zIndex = 111;
			position = "absolute";
			background = "#fff";
			left = "0px";
			top = "0px";
			if (document.all && window.ActiveXObject && !window.opera) {
				filter = "Alpha(Opacity=30)";
			} else {
				opacity = 0.3;
			}
		}
	}
}

function replyboxHide(id){
	if (document.getElementById("divWinBG"))
	{
		document.getElementById("divWinBG").style.display = "none";
	}
	document.getElementById("replybox2").innerHTML = document.getElementById("replybox").innerHTML;
	document.getElementById("replybox").innerHTML = "";
	document.getElementById("replybox").style.display = "none";
}

function toCookie(backurl,str){
	var expdate = new Date();
    var ms = 1*60000*30;//3min
	expdate.setTime(expdate.getTime() + ms);
	setCookie(backurl, str, expdate);
	
}
	function setReplyboxContent(backurl,aid){
		/*
		var backurl = document.getElementById("replyboxurl").value;
		var aa = backurl.substring(backurl.lastIndexOf("/")+1).split(".");
		var aid = aa[0];
		*/
		var aObj = document.getElementById(aid);
		var inCon = getCookie(backurl).substring(0,20);
		if(inCon!=""){
		aObj.innerHTML = getCookie(backurl).substring(0,20)+"...";
		}
	}
	
	function getCookie ( pName )	{

	var search;
	search = pName + "=";
	offset = document.cookie.indexOf(search);
		if ( offset != -1 ) {
			offset += search.length;
			end = document.cookie.indexOf(";", offset);
			if ( end == -1 ) end = document.cookie.length;
				return unescape(document.cookie.substring(offset, end));		
		} else {
			return "";
		}

}

function replyOperate(str){
	toCookie(str);
	replyboxHide();
	setReplyboxContent();
}
var wr_c="";

 function LoadJS( fileUrl ) { 


    var oHead = document.getElementsByTagName('HEAD').item(0); 

   var oScript= document.createElement("script"); 

    oScript.type = "text/javascript"; 

     oScript.src=fileUrl ; 

    oHead.appendChild( oScript); 
}
