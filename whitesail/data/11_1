function greeting() {

         var t = new Date();
	var h = t.getHours();
	var d = t.getDay();
	var l = greeting_w.c;
	if ( h>=6 && h<9 ) {l = l.concat(greeting_w.m);}
	if ( h>=9 && h<12 ) {l = l.concat(greeting_w.a);}
	if ( h>=12 && h<14 ) {l = l.concat(greeting_w.n);}
	if ( h>=14 && h<17 ) {l = l.concat(greeting_w.p);}
	if ( h>=17 && h<19 ) {l = l.concat(greeting_w.d);}
	if ( h>=19 && h<21 ) {l = l.concat(greeting_w.e);}
	if ( h>=21 && h<24 ) {l = l.concat(greeting_w.l);}
	if ( h>=0 && h<6 ) {l = l.concat(greeting_w.w);}
	if (d == 0 || d == 6) {	l = l.concat(greeting_w.k);}
	var n = Math.round(Math.random()*l.length);
	if (n == 0){n = 1;}
	return(l[n-1]);
}	



//passportÿʼ
PassportSC.appid = 1005;
PassportSC.maxIntervalCount = 100;
PassportSC.max_line_length = 24;
PassportSC.registerUrl = "http://profile.chinaren.com/reg/g/i";


var loginMsg = "ӭ¼ ChinaRen!";
var myAppInfoContainer = false;
function drawAppInfo(appInfoContainer){
	myAppInfoContainer = appInfoContainer;
	var userid = PassportSC.cookieHandle();
	var content = '';
	var isIE = window.ActiveXObject?true:false;
	content += '<div class="ppDefWel">';
	if(isIE){
		if(userid.length > 6) userid = userid.substring(0,6)+"..";
		content += '<p style="color:#FF6000;margin-top:5px;text-align:center;">'+userid+'<span id="candleManSpn" style="position:absolute;margin-top:-5px"></span>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;'+greeting()+'</p>';
	}else{
		content += '<p style="color:#FF6000;margin-top:5px;text-align:center;">'+userid+'&nbsp;'+greeting()+'</p>';
	}
	content += '<p style="margin-top:12px;text-align:center;"><a href="http://i.chinaren.com/"><img src="http://www.chinaren.com/upload/sns/default/images/cr_pp_04.gif" border="0"></a> <a href="http://class.chinaren.com"><img src="http://www.chinaren.com/upload/img20070309/cr_pp_01.gif" border="0"></a> <a href="http://club.chinaren.com"><img src="http://www.chinaren.com/upload/img20070309/cr_pp_02.gif" border="0"></a> <a href="http://i.chinaren.com/group"><img src="http://www.chinaren.com/upload/idx080714/cr_pp_03.gif"></a> </p>';
	content += '</div>';
	myAppInfoContainer.innerHTML = content;
	if(isIE){
		var sohuFlash2 = new sohuFlash("http://images.chinaren.com/product/webim/mood/mood.swf?UserID="+PassportSC.cookieHandle(),"candleManSpn","220","110","7");
		sohuFlash2.addParam("quality", "high");
		sohuFlash2.addParam("wmode", "transparent");
		sohuFlash2.addParam("allowscriptaccess", "always");
		sohuFlash2.setAttribute("name", "moodFlash");
		sohuFlash2.write("candleManSpn");
	}
	//webim_config._init();
}
//passportý

function getObject(objectId) {
    if(document.getElementById && document.getElementById(objectId)) {
	// W3C DOM
	return document.getElementById(objectId);
    } else if (document.all && document.all(objectId)) {
	// MSIE 4 DOM
	return document.all(objectId);
    } else if (document.layers && document.layers[objectId]) {
	// NN 4 DOM.. note: this won't find nested layers
	return document.layers[objectId];
    } else {
	return false;
    }	
}

function getParameter(key){
        var cgi = window.location.href;
        var idx = cgi.indexOf("#")
        if(idx != -1){
                cgi = cgi.substring(0,idx);
        }
        idx = cgi.indexOf("?");
        if(idx != -1){
                cgi = cgi.substring(idx+1);
                var parms = new Array();
                if(cgi.indexOf("&") != -1){
                        var spl = cgi.split("&");
                        for(i=0;i<spl.length;i++){
                                if(spl[i].indexOf("=") !=-1){
                                        var part = spl[i].split("=");
                                        parms[part[0]] = part[1];
                                }
                        }
                }else{
                        if(cgi.indexOf("=") !=-1){
                                var part = cgi.split("=");
                                parms[part[0]] = part[1];
                        }
                }
        }
        if(parms && parms[key]) return parms[key];
        return '';
}
</script>
</head>

