<script>
    var yodaoDefault="һҳ";
    var focusFlag = false;
    function inputFocus(e){
        if(e.value== yodaoDefault) {
            e.value='';
        }
        focusFlag = true;
        e.style.color="black";
        e.style.font="14px '',arial";
        e.style.height="21px";
        e.style.paddingTop="3px";
    }
    function checkInput(){
        var e = document.getElementById("query");
        if(e.value==yodaoDefault){
           e.value="";
          return true;
        }
    }
</script>
