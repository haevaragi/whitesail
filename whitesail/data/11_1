//<![CDATA[
var cookie = getAdCookie('sinaVideoAd');
if(cookie==''){
	setAdCookie('sinaVideoAd','played',10);
	initVad();
}
function initVad(){
	try{var c = document.getElementsByTagName("meta")['publishid'].content.split(',')[0];}catch(e){var c=0;}
	document.write('<script language="javascript" type="text/javascript" src="http://dcads.sina.com.cn/html.ng/c='+c+'&site=sina&type=richmedia&loca='+local_index+'&'+Math.random()+'"></scr'+'ipt>');
	document.write('<script language="javascript" type="text/javascript" src="http://d2.sina.com.cn/d1images/cpm/vwin.js"></scr'+'ipt>');
}
function getAdCookie(N){
	var c=document.cookie.split("; ");
	for(var i=0;i<c.length;i++){
		var d=c[i].split("=");
		if(d[0]==N)return unescape(d[1]);
	}
	return '';
}
function setAdCookie(N,V,Q){
	var L=new Date();
	var z=new Date(L.getTime()+Q*60000);
	document.cookie=N+"="+escape(V)+";path=/;expires="+z.toGMTString()+";";
}
//]]>
</script>
