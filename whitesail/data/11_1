function combineswitch(idbase, count, current) {
for(i = 0;i < count;i++) {
$(idbase + '_' + i).style.display = 'none';
$('t' + idbase + '_' + i).className = '';
}
$(idbase + '_' + current).style.display = '';
$('t' + idbase + '_' + current).className = 'ontype';
}
combineswitch('cm_kY91', 3, 0);
