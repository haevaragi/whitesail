function countWorld(obj, maxlength){
	var left = maxlength - obj.value.length;
	if(obj.value.length > maxlength)
    {
		obj.value = obj.value.substring(0, maxlength);
    }
}
function upFun(backurl){

	var aa = backurl.substring(backurl.lastIndexOf("/")+1).split(".");
		
	var article_serial = aa[0];
	var username   = article_serial.substr(0,8);
	var article_id = article_serial.substr(8);
	var digg_src    = 'http://post.tom.com/article_v5.php?action=dig_article&username='+username+'&aid='+article_id+'&timestamp='+new Date().getTime();
//document.write('<img src="'+digg_src+'">');
	var aObj = document.getElementById("replyimage");
	aObj.innerHTML= '<img src="'+digg_src+'">';
	//LoadJS(digg_src);
document.location.reload();

}
function downFun(backurl){
	var aa = backurl.substring(backurl.lastIndexOf("/")+1).split(".");
		
	var article_serial = aa[0];
	var username   = article_serial.substr(0,8);
	var article_id = article_serial.substr(8);
	var trample_src = 'http://post.tom.com/article_v5.php?action=trample_article&username='+username+'&aid='+article_id+'&timestamp='+new Date().getTime();

	var aObj = document.getElementById("replyimage");
	aObj.innerHTML= '<img src="'+trample_src+'">';
         document.location.reload();
}

