function haoSubSearch(sc){
	changePage(sc);
	imglogo=document.getElementById("imglogo");
	if(sc != "baidu"){
		if(sc == "post")
		{
		    f.action = "http://tieba.baidu.com/f";
			imglogo.src="logo_post.gif";
			imglogo.alt="ٶ";
			urllogo.href="http://tieba.baidu.com/";
		}
		else if(sc == "zhidao")
		{
			f.action = "http://zhidao.baidu.com/q";
			imglogo.src="logo_zhidao.gif";
			imglogo.alt="ٶ֪";
			urllogo.href="http://zhidao.baidu.com/";
		}
		else if(sc == "mp3")
		{
			f.action = "http://mp3.baidu.com/m";
			imglogo.src="logo_mp3.gif";
			imglogo.alt="ٶMP3";
			urllogo.href="http://mp3.baidu.com/";
		}
		else if(sc == "img")
		{
			f.action = "http://image.baidu.com/i";
			imglogo.src="logo_pic.gif";
			imglogo.alt="ٶͼƬ";
			urllogo.href="http://image.baidu.com/";
		}
		else if(sc == "news")
		{
			f.action = "http://news.baidu.com/ns";
			imglogo.src="logo_news.gif";
			imglogo.alt="ٶ";
			urllogo.href="http://news.baidu.com/";
		}
		else if(sc == "Video")
		{
			f.action = "http://video.baidu.com/v";
			imglogo.src="logo_video.gif";
			imglogo.alt="ٶƵ";
			urllogo.href="http://video.baidu.com/";
		}
		else
		{
			f.action = "http://www.baidu.com/s";
			imglogo.src="logox3.gif";
			imglogo.alt="ٶҳ";
			urllogo.href="http://www.baidu.com/index.php?tn=sitehao123";
		}
		f.sc.value = sc;
	}
	else{
		f.sc.value = "post";
		haoGoSoba();
	}
}

function haoGoSoba(){

	f.action = "http://www.baidu.com/s";
	f.kw.value = f.word.value;
}
function G(id)
{
 return document.getElementById(id);
}
function changePage(sc){
	if(haoCurrHighLight == null){
		haoCurrHighLight = document.getElementById("haoweb");
	}
	haoCurrHighLight.className = "searchfod2";
	haoCurrHighLight = document.getElementById("hao"+sc);
	haoCurrHighLight.className = "searchfod1";
	document.f.word.focus()
	 f.word.value="";
}

function isSoba(){
	if(haoCurrHighLight == document.getElementById("haosoba")){
		haoGoSoba();
		return true;
	}
	else{
		return true;
	}
}

function goSearch(){
	changeHotWords(document.f.sc.value);
	if(!haoValidate(1))
	{
			return false;
			}
	else{
	 return false;
	 }
}

function changeHotWords(category){
	document.getElementById("td_hotlist").innerHTML = eval(category + "Words");
}
function showData(id)
{
    var stock = getCookie("HAOSTOCK");
    var lottery = getCookie("HAOLOTTERY");
    var sStock=sLottery="";
    if(stock!=null||lottery!=null)
    {
        var stockData = eval(stock);
        var lotteryData = eval(lottery);
        for(var i in stockData)
        {
            sStock+="<a href="+stockData[i].url+" target=_blank>"+stockData[i].name+"</a>&nbsp;&nbsp;";
        }
        for(var i in lotteryData)
        {
           sLottery+="<a href="+lotteryData[i].url+" target=_blank>"+lotteryData[i].name+"</a>&nbsp;&nbsp;";
        }
        G(id).innerHTML="<b><a id=mylikeopen href=javascript:void(0) onclick=\"haoGX.openGX();event.returnValue=false;return false;\"> ҶƵվ</a></b>"+sStock+sLottery;
    }else
    {
        G(id).style.display="none";
    }
}
