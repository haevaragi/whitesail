function sn(n)
{
	var urlone = "http://alumni.chinaren.com";
	var urltwo = "http://www.chinaren.com";
	var urlthree = "http://www.sogou.com";

	for (i=2;i<=14;i+=2) {
		if (i == n) {
			getObject("z1_"+i).className="libg";
			getObject("z1_"+(i-1)).style.display="none";
			getObject("z1_"+(i+1)).style.display="none";
			if(n!=2) getObject("z1_1").style.display="";
		}
		else
		{
			getObject("z1_"+i).className="";
			getObject("z1_"+(i+1)).style.display="";
		}
	}

	var str1 = getObject('sogoulogoPic').src;

	var postUrl = "";
	var postKey = "";
	var postKeyValue = "";
	switch (n) {
		case 2:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo06.gif";
			getObject("logoPicUrl").href= urlone;
			postUrl = "http://search.chinaren.com/alusearch/school_search.jsp?prov=-1&schtype=-1";
			postKey = "keyword";

			break;
		case 4:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo06.gif";
			getObject("logoPicUrl").href= urlone;
			postUrl = "http://search.chinaren.com/alusearch/class_search.jsp";
			postKey = "keyword";
			postKeyValue = "༶  ؼ";
			break;
		case 6:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo06.gif";
			getObject("logoPicUrl").href= urlone;
			postUrl = "http://search.chinaren.com/alusearch/cm_search.jsp";
			postKey = "keyword";
			postKeyValue = "ͬѧ֣ÿոֿȷ";
			break;
		case 8:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo08.gif";
			getObject("logoPicUrl").href= urltwo;
			postUrl = "http://club.chinaren.com/query/query_adv.jsp?type=3";
			postKey = "key";
			postKeyValue = "ȤĻ⣬ǰھྫ :)";
			break;
		case 10:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo07.gif";
			getObject("logoPicUrl").href= urlthree;
			postUrl = "http://www.sogou.com/web";
			postKey = "query";
			postKeyValue = "";
			break;
		case 12:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo07.gif";
			getObject("logoPicUrl").href= urlthree;
			postUrl = "http://d.sogou.com/music.so?class=1&sid=";
			postKey = "query";
			postKeyValue = "  ";
			break;
		case 14:
			getObject('sogoulogoPic').src = "http://www.chinaren.com/upload/idx20080401/images/logo07.gif";
			getObject("logoPicUrl").href= urlthree;
			postUrl = "http://pic.sogou.com/pics?mode=1";
			postKey = "query";
			postKeyValue = "ϲǡ ǰͼƬ";
			break;
	}
	if(n==2){
		init_js( 'searchFM', 'searchKEY', 'http://search.chinaren.com/alusearch/suggest.jsp');
		// Run main loop
		setTimeout("main_loop()", 10 );
	}else{
		clearTimeout(V);
		getObject('searchKEY').onfocus = function() {this.value='';};
	}	
	getObject('searchFM').action = postUrl;
	getObject('searchKEY').name = postKey;
	getObject('searchKEY').value = postKeyValue;

	var kp = getObject("kwl" + n).innerHTML.split(" ");
	var ostr = "";
	for (var i=0; i<kp.length; i++) {
		if (kp[i].length>0) {
			if (postUrl.indexOf('?')>0) 
				ostr += "<a href=" + postUrl + "&" + postKey + "=" + kp[i] + " target='_blank'>" + kp[i] + "</a> ";
			else
				ostr += "<a href=" + postUrl + "?" + postKey + "=" + kp[i] + " target='_blank'>" + kp[i] + "</a> ";
		}
	}
	getObject('stext').innerHTML = ostr;
}

