<script type="text/javascript" src="http://tom.com/weather/weather.php"></script>

<script type="text/javascript">
(function() {
  function cumulativeOffset(elem) {
    var x = 0, y = 0;
    do {
      y += elem.offsetTop  || 0;
      x += elem.offsetLeft || 0;
      elem = elem.offsetParent;
    } while (elem);
    return {left: x, top: y};
  }
  
  var huangliBtn = document.getElementById("huanglibtn");
  var huangliDetail = document.getElementById("huanglidetail");
  
  var huangliDetailStatus = {
    hasShow: false,
    timer: null,
    fnHideTimer: function() {
      huangliDetail.style.display = "none";
    },
    show: function() {
      var hlBtnPos = cumulativeOffset(huangliBtn);
      huangliDetail.style.display = "block";
      huangliDetail.style.left = hlBtnPos.left + huangliBtn.offsetWidth - huangliDetail.offsetWidth + "px";
      huangliDetail.style.top = hlBtnPos.top + huangliBtn.offsetHeight + 2 + "px";
    }
  };
  
  huangliBtn.onmouseover = function() {
    huangliDetailStatus.show();
    if (huangliDetailStatus.timer) {
      clearTimeout(huangliDetailStatus.timer);
      huangliDetailStatus.timer = null;
    }
  }
  huangliBtn.onmouseout = function() {
    if (!huangliDetailStatus.timer) {
      huangliDetailStatus.timer = setTimeout(huangliDetailStatus.fnHideTimer, 700);
    }
  }
  huangliDetail.onmouseover = function() {
    if (huangliDetailStatus.timer) {
      clearTimeout(huangliDetailStatus.timer);
      huangliDetailStatus.timer = null;
    }
  }
  huangliDetail.onmouseout = function() {
    if (!huangliDetailStatus.timer) {
      huangliDetailStatus.timer = setTimeout(huangliDetailStatus.fnHideTimer, 100);
    }
  }
  
  //自动显示皇历
  huangliDetailStatus.show();
  
  //3s后关闭皇历
  setTimeout(function() {
    huangliDetail.style.display = "none";
  }, 3000);
})();
</script>

