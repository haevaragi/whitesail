<script language=javascript>
 function dooSubmit() 
 {
 		objbrand=document.getElementById('brand');
		var indexbrand=objbrand.selectedIndex; 
		var valbrand =objbrand.options[indexbrand].value;
		var allBrandName=objbrand.options[indexbrand].text;
		
		if(valbrand==-1)
		{
			alert("ȷѡƷƣ");  
		}
		else
		{
			var temp=allBrandName.split(" ");
  		var brandName=temp[1];
			obj=document.getElementById('model');
			var index=obj.selectedIndex;   
			var val = obj.options[index].value;
			if(val == "-1"){   
				document.SBL.action = "http://db.auto.sohu.com/searchText.sip?item="+escape(brandName)+"&pageNo=1&module=AutoModel&type=text";
			}else{ 
				document.SBL.action = "http://db.auto.sohu.com/model-"+val+".shtml"; 
			} 
			document.SBL.submit();	
		}		
 }        
 
  function  showBrandFromJs(){
  
    var obj=document.getElementById('brand');
    obj.options.length=0;                     
    obj.options.add(new Option("--ѡƷ--","-1")); 
   
   
   for(var i=0;i<brand.length;i++){   
   if(brand[i]!=null){
   var result =brand[i].split(";");
   obj.options.add(new Option(result[1],result[0]));    
  }
  }
  
  }
  
 function getModelJs(){
  var objbrand=document.getElementById('brand');
  var objmodel=document.getElementById('model')
  var indexbrand=objbrand.selectedIndex-1; 
  var modelCount=0;
   objmodel.options.length=0;
   objmodel.options.add(new Option("--ѡ--","-1")); 
  while (model[indexbrand][modelCount]!=null){
  modelCount++;
  
  }
  for (var i=0;i<=modelCount-1;i++){
  var result=model[indexbrand][i].split(",");
  objmodel.options.add(new Option(result[1],result[0]));   
  }
  }

</script>
