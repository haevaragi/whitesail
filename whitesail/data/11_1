{c="#093";}
if(i==0)
{h+="<div><a href='http://stockhtm.finance.qq.com/hqing/zhishu/000001.htm' target='_blank'>ָ֤</a> ";v_zqdm="000001";}
else if(i==1)
{h+="<div><a href='http://stockhtm.finance.qq.com/hqing/zhishu/399001.htm' target='_blank'>ָ֤</a> ";v_zqdm="399001";}
