function zhishucallback()
{if(typeof(v_qq_hq)!="undefined")
{if(v_qq_hq!="")
{var v=v_qq_hq,h='';for(var i=0;i<v.length&&i<3;i++)
{var c="#000",t='',v_zqdm="";if(parseFloat(v[i][1],10)>0)
{c="#ff0000";t='+';}
