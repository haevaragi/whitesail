		var article_serial = aa[0];
		
		//toCookie(backurl,str);
		replyboxHide();
		var content= str;
		
		
		

    //对内容进行编码
   content = escape(content);
    //写入的内容
     wr_c = "<script type='text/javascript' src='http://post.tom.com/index_voice.php?action=ajax_submit_reply&article_serial="+article_serial+"&reply_body="+content+"'><\/script>";

    //document.write(wr_c);
    var src="http://post.tom.com/index_voice.php?action=ajax_submit_reply&article_serial="+article_serial+"&reply_body="+content;
  	LoadJS(src);
    //setReplyboxContent(backurl,"mo_con"+backurl);
    
}
