<script type="text/javascript">
	function $E(e){return document.getElementById(e);}
	function submitRequest(a){
		var b=a;
		var c=$E("chgmobile");
		var d=$E("chgmobile2");
		if(c.value==""){
			alert("ֻ롣");
			c.focus();
			return;
		}
		if(d && d.value==""){
			alert("ȷֻ롣");
			d.focus();
			return;
		}
		if(!c.value.match(/^(13[0-9]{9}|15[0-9]{9})$/)){
			alert("ֻ벻ȷ룡");
			c.focus();
			return;
		}
		if(d && c.value!=d.value){
			alert("ֻ벻һ£룡");
			d.focus();
			return;
		}
		var e=$E("chargeAutoPrix").value;
		if(e=="30000"&&c.value.match(/^(13[0-3][0-9]{8}|153[0-9]{8})$/)){
			alert("ͨݲ֧300Ԫֵ");
			$E("chargeAutoPrix").focus();
			return;
		}
		para="tid=01001001&anonyuser=1&uin="+c.value+"&chgmobile="+c.value+"&amount="+e+"&chargeway="+b+"&bankid=0&PTAG=30053.20.8&attach=paipai";
		window.open("http://pay.paipai.com/cgi-bin/pay_mobile_card?"+para);
	};
	function chargePrix(){
		var curPrix = $E("chargeAutoPrix").value;
		var beginPrice = ((curPrix*0.983)/100).toFixed(2),endPrice = ((curPrix*0.988)/100).toFixed(2);
		if(curPrix>10000){beginPrice = ((curPrix*0.983)/100).toFixed(1),endPrice = ((curPrix*0.988)/100).toFixed(1);}
		var priceEM = $E("chargeMobileTab").getElementsByTagName("em");
		priceEM[0].innerHTML = beginPrice+"-"+endPrice;
	}
</script>
