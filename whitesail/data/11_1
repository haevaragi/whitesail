<script>
var t=setInterval(myfunc,1000)
var d2=document.getElementById("div2")
function myfunc(){
var o2=d2.firstChild
d2.removeChild(o2)
d2.appendChild(o2)
}
d2.onmouseover=function(){clearInterval(t)}
