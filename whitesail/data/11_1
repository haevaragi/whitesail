//to speed up the login page
//if cause error mail to lu.hua@opi-corp.com
function fileLoader()
{
var ci = 0;
var files = [];
function loadFiles()
{
if ( ci == files.length ) return;
var o = files[ ci ];
if ( !o ) return;
var This = arguments.callee;
function onload()
{
try
{
if ( o.c ) o.c();
}catch(e){}
This();
}
if ( /\.js(\?|$)/.test( o.f ) )
{
var el = document.createElement( 'script' );
el.type="text/javascript";
el.src = o.f;
if ( !!( window.attachEvent && !window.opera ) )
{
el.onreadystatechange = function()
{
if ( (this.readyState == 'loaded' || this.readyState == 'complete' ) && !this.hasLoad )
{
this.hasLoad = true;
onload();
}
}
}else{
el.onload = onload;
}
document.body.appendChild( el );
}
else if ( /\.css(\?|$)/.test( o.f ) )
{
var el = document.createElement( 'link' );
el.rel = 'stylesheet';
el.type = 'text/css';
el.href = o.f;
document.body.appendChild( el );
This();
}
ci ++;
}
this.add = function( file )
{
files.push( file );
};
this.load = function()
{
loadFiles();
};
}
(function()
{
var ld = new fileLoader();
//  ld.add({f:'http://xnimg.cn/117629/jspro/xn.core.js'});
//  ld.add({f:'http://xnimg.cn/117291/jspro/xn.ui.js'});
//  ld.add({f:'http://xnimg.cn/14050/js/yui/yahoo/yahoo-min.js'});
//  ld.add({f:'http://xnimg.cn/14050/js/yui/event/event-min.js'});
//  ld.add({f:'http://xnimg.cn/14050/js/yui/yahoo-dom-event/yahoo-dom-event.js'});
//  ld.add({f:'http://xnimg.cn/14050/js/yui/connection/connection-min.js'});
//  ld.add({f:'http://xnimg.cn/69414/js/yui/autocomplete/autocomplete-min.js'});
//  ld.add({f:'http://xnimg.cn/autoComplete.js?ver=$revxxx$'});
ld.add({f:'http://xnimg.cn/16312/js/cityArray.js'});
ld.add({f:'http://xnimg.cn/110246/js/schoolComponent.js'});
ld.add({f:'http://xnimg.cn/97759/js/login.js'});
ld.add({
f:'http://xnimg.cn/102186/js/register/registerutil-syshome.js',
c:function()
{
regSelect = _regSelect;
document.getElementById('name').onblur = function(){
ajaxCheckName();
}
document.getElementById('regEmail').onblur = function(){
ajaxCheckEmail();
}
}
});
ld.add({
f:'http://xnimg.cn/53317/js/fillinfo.js',
c:function()
{
Profile.getInCityListOfProvince($("homeprovince").selectedIndex, 'homecitycode');
}
});
ld.add({f:'http://xnimg.cn/104853/js/inCityArray.js'});
ld.add({f:'http://xnimg.cn/111157/js/util.js'});
ld.add({f:'http://xnimg.cn/1188/js/prototype.js'});
ld.add({f:'http://xnimg.cn/68471/js/searchschlist.js'});
ld.add({
f:'http://xnimg.cn/js/allunivlist.js?ver=$revxxx$',
c:function()
{
univtabs.changeCountry('0');
}
});
loadRegFile = function( e )
{
var el = e.target || e.srcElement;
if(el.tagName.toLowerCase() != 'input')return;
if(arguments.callee.hasload)return;
arguments.callee.hasload = true;
ld.load();
}
})();
//speed up code end
var checkICode = true;
regSelect = _regSelect = function()
{
var stage = document.getElementById("stage").value;
try{
var o1=document.getElementById("p_highSchoolName0");
var o2 = document.getElementById("p_juniorHighSchoolName");
var o3 = document.getElementById("p_techHighSchoolName");
if(o1)
o1.style.display = "none";
if(o2)
o2.style.display = "none";
if(o3)
o3.style.display = "none";
}catch(e)
{}
if(stage == 20) {
$("d1").style.display = "block";
$("d2").style.display = "none";
$("typeOfCoursespan").style.display = "block";
$("d_workplace").style.display = "none";
} else if(stage == 10) {
$("d1").style.display = "none";
$("d2").style.display = "block";
$("typeOfCoursespan").style.display = "none";
$("d_workplace").style.display = "none";
$("schoolType").value = "-1";
} else if(stage == 30 || stage == 90) {
$("d1").style.display = "none";
$("d2").style.display = "none";
$("typeOfCoursespan").style.display = "none";
$("d_workplace").style.display = "block";
}
}
function ajaxRegisterLog(){
var myDate = new Date();
var email = $("regEmail").value;
var ss = $("ss").value;
var url = '/AjaxRegisterLog.do';
var pars = 'action_id=200061&email=' + email + '&s1=' + ss + '&t=' + myDate.getTime();
var myAjax = new Ajax.Request(url, {
method :'post',
parameters :pars
});
}function refreshCode(){
var imgUrl = "http://icode.xiaonei.com/getcode.do?t=register_206675568&rnd=1242038522412";
$('verifyPic').src = imgUrl + Math.random();
}
function init() {
regSelect();
}
function clsearch(e) {
if (!e) var e = window.event;
if (e.target) targ = e.target;
else if (e.srcElement) targ = e.srcElement;
if (targ.nodeType == 3) // defeat Safari bug
targ = targ.parentNode;
var schName = targ.innerHTML; 
var link= targ.href;
var startPos = link.indexOf("id=");
if(startPos == -1) {
alert("链接错误");
return false;
}
else {
startPos += 3;
var schId = link.substring(startPos);
}
$("universityid").value = schId;
$("universityname").value = schName;
hidepan();
regSelect();
return false;
}
//===============
