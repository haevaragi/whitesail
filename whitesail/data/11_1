<script>
$('#ddmenu').ready(function() {
if($.browser.msie){$('#ddmenu > li').each(function(){$(this).hover(function(){$(this).addClass('over')}, function(){$(this).removeClass('over')})});}
});
$(document).ready(function(){
$("input[@type='text'], input[@type='password'], input[@type='file'], textarea").focus(function(){$(this).css({background:"#FFFFF2"})}).blur(function(){$(this).css({background:"transparent"})});

function getUrlParam(param){
var hrefstr,pos,parastr,para,tempstr;
hrefstr = self.location.href;
pos = hrefstr.indexOf("?");
end = hrefstr.indexOf("#");
if(end<0)end = hrefstr.length;
parastr = hrefstr.substring(pos+1,end);
para = parastr.split("&");
tempstr="";
for(i=0;i<para.length;i++){
  tempstr = para[i];
  pos = tempstr.indexOf("=");
  if(tempstr.substring(0,pos) ==param){
    return tempstr.substring(pos+1);
  }
}
return null;
}

<!-- ޸ҳת -->
  $('input[@name=custompage]').each(function() {
    $(this).unbind().keydown(function(ev) {
      var e = ev ? ev : (window.event ? window.event : null);
      if (e.keyCode == 13) {
        if (self.location.href.indexOf('thread') >= 0) {
          window.location.href = 'thread-10139635-' + $(this).val() + '-1.html';
        } else if (self.location.href.indexOf('forum') >= 0) {
          window.location.href = 'forum-84-' + $(this).val() + '.html';
        } else if (self.location.href.indexOf('pm.php?folder=') >= 0) {
          var folder = getUrlParam('folder');
          window.location.href = 'pm.php?folder=' + folder + '&page=' + $(this).val();
        }
        return false;
      }
    });
  });

})
</script>
