<script type="text/javascript" >
	function switch_to(id){
	var item = new Array("film","blog","topic");
	var i = 0;
	 for(i=0;i<item.length;i++){
      if(id == item[i])
		 {
		 document.getElementById(item[i]).className = "onselect";
		 document.getElementById(item[i]+"_zone").style.display = "";
		 }
		 else
		 {
		 document.getElementById(item[i]).className = "offselect";
		 document.getElementById(item[i]+"_zone").style.display = "none";
		 }	
	 }
	}
</script>
