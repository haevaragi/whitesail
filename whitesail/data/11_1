textLinks.push(ad);

//---------- AE  ----------

var ary = new Array();
var date = new Date();
for(var i=0; i<adLinks.length; i++){
	var start = strToDate(adLinks[i].startdate.replace('<startdate>','').replace('</startdate>',''));
	var end = strToDate(adLinks[i].enddate.replace('<enddate>','').replace('</enddate>',''),true);
	if(date>start && date<end || start==null){
		ary.push(adLinks[i]);
	}
}
var n = adLinks.num-ary.length;
for(var i=0; i<n; i++){
	ary.push(textLinks[i%textLinks.length]);
}
var id = globalRotatorId%adLinks.num;
for(var i=0; i<ary[id].length; i++){
	document.write('<a href="'+ary[id][i][1]+'"  target="_blank">'+ary[id][i][0]+'</a>'+(i==0?'| ':(i<ary[id].length-1?' ':'')));
}
function strToDate(str,ext){
	if(str==null || str=='') return;
	var arys = new Array();
	arys = str.split('-');
	var newDate = new Date(arys[0],arys[1]-1,arys[2],9,0,0);
	if(ext){
		newDate = new Date(newDate.getTime()+1000*60*60*24);
	}
	return newDate;
}
})()
