<script type="text/javascript">
var ppp = '';

$(document).keydown(function(ev) {
if(ev.target.tagName == "INPUT" && ev.target.type == "text") return true;
if(ev.target.tagName == "INPUT" && ev.target.type == "file") return true;
if(ev.target.tagName == "SELECT" ) return true;
if(ev.target.tagName == "TEXTAREA" ) return true;
if(ev.ctrlKey) return true;
switch (ev.keyCode) {
case 13:
if (ppp != '') document.location = "thread-10139635-" + ppp + "-1.html";
break;
case 78:
document.location = "post.php?action=newthread&fid=84&fpage=1";
break;
case 77:
document.location = "pm.php?folder=inbox";
break;
case 80:
document.input.message.focus();
break;
case 72:
document.location = "home.php";
break;
case 70:
document.location = "forum-84-1.html";
break;
case 73:
document.location = "index.php";
break;
case 76:
$('div.t_smallfont').toggleClass('t_bigfont');
$('a.hand').each(function() {$(this).text()==''?$(this).text('С'):$(this).text('')});
break;
case 82:
document.location = "post.php?action=reply&fid=84&tid=10139635&fpage=1";
break;
case 39:
if (ev.ctrlKey)
document.location = "redirect.php?fid=84&tid=10139635&goto=nextnewset";
else
document.location = "thread-10139635-2-1.html";
break;
case 37:
if (ev.shiftKey)
document.location = "thread-10139635-1-1.html";
else if (ev.ctrlKey)
document.location = "redirect.php?fid=84&tid=10139635&goto=nextoldset";
else
document.location = "thread-10139635-1-1.html";
break;
}

if (ev.keyCode <= 57 && ev.keyCode >= 48) ppp = ppp + String.fromCharCode(ev.keyCode);
else if (ev.keyCode <= 105 && ev.keyCode >= 96) ppp = ppp + String.fromCharCode(ev.keyCode - 48);

return true;
});
</script>

