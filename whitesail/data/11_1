{return 0;}}
else
{return 2;}}
function fm_submit()
{var value=document.getElementById("cl").value;if(CheckStockInput2(value)==1)
{alert("ѯ//ƴ");return false;}
else if(CheckStockInput2(document.getElementById("cl").value)==2)
