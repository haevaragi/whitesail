pictures[5][2] = {image: "http://i1.tomoimg.cn/uldf/2009/0511/zhanglingling/aiwan2.jpg",alt: "山花烂漫", link: "http://photo.tom.com/picture/517441490/23695.html?source=HP_HAPPY", grade: 8};
pictures[5][3] = {image: "http://i1.tomoimg.cn/uldf/2009/0511/zhanglingling/aiwan3.jpg",alt: "高原雪人", link: "http://photo.tom.com/picture/155727619/9305.html?source=HP_HAPPY", grade: 9};

				var sTabs = "";
				for (var i = 1; i <= tabs.length - 1; i++) {
				  if (tabs[i].isDefault) {
					sTabs += '<li class="current" id="happy'+(i)+'" onmouseover="tab(\'happy\','+(i)+','+(tabs.length - 1)+')">'+tabs[i].title+'</li>';
				  } else {
					sTabs += '<li id="happy'+(i)+'" onmouseover="tab(\'happy\','+(i)+','+(tabs.length - 1)+')">'+tabs[i].title+'</li>';
				  }
				}

				var sPictures = '';
				for (var i = 1; i <= pictures.length - 1; i++) {
				  if (tabs[i].isDefault) {
					sPictures += '			<div class="happytxt" id="happy_cont'+i+'">';
				  } else {
					sPictures += '			<div class="happytxt" id="happy_cont'+i+'" style="display:none;">';
				  }
				  var ps = pictures[i];
				  for (var j = 1; j <= ps.length - 1; j++) {
					var sGrade = "";
					var grade = ps[j].grade;
					for (var k = 0; k < Math.ceil(grade / 2) - 1; k++) {
					  sGrade += '<img src="http://www.tom.com/images/2009/heart.gif" alt="" />';
					}
					if (grade % 2 != 0) {
					  sGrade += '<img src="http://www.tom.com/images/2009/banheart.gif" alt="" />';
					} else {
					  sGrade += '<img src="http://www.tom.com/images/2009/heart.gif" alt="" />';
					}
					for (var k = 0; k < (5 - Math.ceil(grade / 2)); k++) {
					  sGrade += '<img src="http://www.tom.com/images/2009/grayheart.gif" alt="" />';
					}
					sPictures += '\
								<dl>\
									<dt><a href="'+ps[j].link+'" target="_blank"><img src="'+ps[j].image+'" alt="'+ps[j].alt+'" /></a></dt>\
									<dd>得分：<em>'+grade+'</em>分</dd>\
									<dd>'+sGrade+'</dd>\
								</dl>\
								';
				  }
				  sPictures += '			</div>';
				}

				var s = ' \
							<ul>\
								' + sTabs + '\
							</ul>\
							'+sPictures;


				document.write(s);
				})();

            //-->
            </SCRIPT>
        </div>
        <!--}end happy-->
		<!--}end happy-->
